
{
  "id": "panel",
  "class": "panel",
  "title": "PCjs Debugger",
  "style": {
    "width": "auto",
    "max-width": "100%",
    "padding-top": "8px"
  },
  "controls": [
    {
      "type": "group",
      "class": "pcjs-panel"
    },
    {
      "id": "print",
      "type": "textarea",
      "height": "260px",
      "style": {
        "width": "100%"
      }
    },
    {
      "id": "debugInput",
      "type": "text",
      "style": {
        "clear": "both"
      }
    },
    {
      "id": "debugEnter",
      "type": "button",
      "label": "Enter"
    },
    {
      "id": "clear",
      "type": "button",
      "label": "Clear"
    },
    {
      "id": "run",
      "type": "button",
      "label": "Run",
      "style": {
        "padding-left": "8px"
      }
    },
    {
      "id": "step",
      "type": "button",
      "label": "Step"
    },
    {
      "id": "setSpeed",
      "type": "button",
      "label": "4.77Mhz"
    },
    {
      "type": "description",
      "label": "Speed:",
      "style": {
        "padding-bottom": "8px",
        "padding-left": "8px"
      }
    },
    {
      "id": "speed",
      "type": "description",
      "label": "Stopped",
      "style": {
        "padding-left": "4px"
      }
    },
    {
      "type": "group",
      "style": {
        "padding-left": "8px",
        "width": "280px"
      }
    },
    {
